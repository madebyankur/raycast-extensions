{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "finder-helpers",
  "title": "Finder Helpers",
  "description": "Helpers for finder",
  "icon": "command-icon.png",
  "author": "madebyankur",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Finder Context Menu",
      "subtitle": "Finder Helpers",
      "description": "Open finder context menu from Raycast",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.2",
    "@raycast/utils": "^1.13.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
